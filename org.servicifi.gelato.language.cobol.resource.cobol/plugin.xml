		</super>
	</extension>
	
	<extension id="org.servicifi.gelato.language.cobol.resource.cobol.problem.batch_constraint_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.servicifi.gelato.language.cobol.resource.cobol.problem">
		</super>
	</extension>
	
	<extension id="org.servicifi.gelato.language.cobol.resource.cobol.problem.live_constraint_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
